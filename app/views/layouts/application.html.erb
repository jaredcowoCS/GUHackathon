<!DOCTYPE html>
<html>
<head>
  <title>Beltraide</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>

<body class="primary">

	<%= render 'header' %>

	<%= render 'messages' %>

	<!-- Main Content Section -->
	<div class="container main">
		<%= yield %>	
	</div>

	<!-- Footer Section -->
	<%= render 'footer' %>

</body>

</html>
